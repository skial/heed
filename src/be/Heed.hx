    public static final regexAsciiWhitelist:EReg = new EReg('[\\u0001-\\u007F]'.translate(), 'ug');

    public static final escapeRegex = ~/["&'<>`]/g;
    public static final escapeMapKeys:Array<String> = ['"', '&', '\'', '<', '>', '`'];
    public static final escapeMapValues:Array<String> = [quot, amp, '&#x27;', lt, gt, '&#x60;'];

    public static inline function escape(value:String):String {
        return escapeRegex.map( value, e -> escapeMapValues[escapeMapKeys.indexOf( e.matched(0) )] );
    }

    #if php
    private static function phpMap(ereg:EReg, value:String, f:EReg->String):String {
        var result = value;
        while (ereg.match(result)) {
            result = ereg.matchedLeft() + f(ereg) + ereg.matchedRight();
        }
        return result;
    }
    #end

    public static function encode(value:String, everything:Bool = false, named:Bool = false, decimal:Bool = false, unsafe:Bool = false):String {
        if (everything) {
            #if !php
            value = regexAsciiWhitelist.map(value, function(ereg) {
                var str = ereg.matched(0);
                return if (named && entityHash.exists(str)) {
                    '&' + entityHash.get(str) + ';';

                } else {
                    var symbol = str.charCodeAt(0);
                    decimal ? (symbol:Decimal) : (symbol:Hex);
                    
                }
            });
            #else
            if (regexAsciiWhitelist.match(value)) {
                var buf = new StringBuf();

                for (idx in 0...value.length) {
                    var symbol = value.charCodeAt(idx);

                    if (symbol >= 0x01 && symbol <= 0x7F) {
                        if (named && entityHash.exists(value.charAt(idx))) {
                            buf.add('&' + entityHash.get(value.charAt(idx)) + ';');

                        } else {
                            buf.add( ((decimal ? (symbol:Decimal) : (symbol:Hex)):String) );
                            
                        }
                    } else {
                        buf.addChar(symbol);

                    }

                }

                value = buf.toString();
            }
            
            #end

            if (named) {
                value = new EReg('&gt;\\u20D2'.translate(), 'ug').replace(value, nvgt);
                value = new EReg('&lt;\\u20D2'.translate(), 'ug').replace(value, nvlt);
                value = new EReg('&#x66;&#x6A;', 'ug').replace(value, fjlig);
                
            }

            if (named) {
                value = regexEncodeNonAscii
                #if php .phpMap #else .map #end( value,  e -> {
                    '&' + entityHash.get( e.matched(0) ) + ';';
                } );
                
            }

        } else if (named) {
            if (!unsafe) {
                value = escapeRegex
                .map( value, e -> '&' + entityHash.get( e.matched(0) ) + ';' );

            }

            value = new EReg('&gt;\\u20D2'.translate(), 'ug').replace(value, nvgt);
            value = new EReg('&lt;\\u20D2'.translate(), 'ug').replace(value, nvlt);

            value = regexEncodeNonAscii
            #if php .phpMap #else .map #end
            ( value, ereg -> {
                var c = ereg.matched(0);
                '&' + entityHash.get( c ) + ';';
            } );

        } else if (!unsafe) {
            value = escapeRegex.map( value, ereg -> {
                var symbol = unifill.InternalEncoding.codePointAt(ereg.matched(0), 0);
                decimal ? (symbol:Decimal) : (symbol:Hex);
            } );

        }
        
        value = be.heed.macros.Util.get_regexAstralSymbol()
        #if php .phpMap #else .map #end( value, e -> {
            var char = e.matched(0);
            var codepoint = unifill.InternalEncoding.codePointAt(char, 0);
            decimal ? (codepoint:Decimal) : (codepoint:Hex);
        }) ;
        
        var regexBmpWhitelist = be.heed.macros.Util.get_regexBmpWhitelist();
        value = regexBmpWhitelist
        #if php .phpMap #else .map #end( value, e -> {
            var symbol = unifill.InternalEncoding.codePointAt(e.matched(0), 0);
            decimal ? (symbol:Decimal) : (symbol:Hex);
        } );
        
        return value;
    }

    public static function decode(value:String, isAttribute:Bool = false, strict:Bool = false):String {
        return regexDecode.map( value, function(ereg) {
            var ref:Null<String> = null;
            var next:Null<String> = null;
            var digit:Null<String> = null;

            if ((ref = ereg.matched(1)) != null) {
                return (cast ref:HtmlEntity).asCodePoints().fromCodePoints();

            }

            if ((ref = ereg.matched(2)) != null) {
                next = ereg.matched(3);

                if (next != null && next != '' && isAttribute) {
                    if (strict && next == '=') {
                        throw '`&` did not start a character reference';
                    }
                    return ereg.matched(0);

                } else {
                    if (strict) throw 'named character reference was not terminated by a semicolon';
                    if (decodeLegacyKeys.indexOf(cast ref) > -1) {
                        return (cast ref:HtmlEntity)
                            .asCodePoints()
                            .fromCodePoints()
                            .toString() +
                            (next != null ? next : '');
                    }

                }

            }

            if ((digit = ereg.matched(4)) != null) {
                // Decode decimal escapes, e.g. `&#119558;`.
                var simi = ereg.matched(5);
                if (strict && (semi == null || semi == '')) throw 'character reference was not terminated by a semilcolon';
                if (digit.length > 6) digit = digit.substring(0, 6);
                var codepoint = Std.parseInt(digit);
                if (codepoint == null) codepoint = 0;
                if (codepoint < 0) codepoint = -codepoint;
                return codePointToSymbol(codepoint, strict);
            }
            
            if ((digit = ereg.matched(6)) != null) {
                // Decode hexadecimal escapes, e.g. `&#x1D306;`.
                var semi = ereg.matched(7);
                if (strict && (semi == null || semi == '')) throw 'character reference was not terminated by a semilcolon';
                if (digit.length > 6) digit = digit.substring(0, 6);
                var codepoint = Std.parseInt('0x$digit');
                if (codepoint == null) codepoint = 0;
                if (codepoint < 0) codepoint = -codepoint;
                return codePointToSymbol(codepoint, strict);
            }

            if (strict) throw 'named character reference was not terminated by a semicolon';
            
            return ereg.matched(0);
        } );
    }

    public static function codePointToSymbol(codepoint:Int, strict:Bool = false):String {
        var result = '';

        if ((codepoint >= minHighSurrogate && codepoint <= maxLowSurrogate) || codepoint > maxCodePoint) {
            if (strict) {
                throw 'character reference outside the permissible Unicode range';
            }
            return '\uFFFD';
        }

        var index = 0;
        if ((index = decodeMapNumericKeys.indexOf(codepoint)) > -1) {
            if (strict) {
                throw 'disallowed character reference';
            }
            return decodeMapNumericValues[index];
        }

        if (strict && invalidReferenceCodePoints.indexOf(codepoint) > -1) {
            throw 'disallowed character reference';
        }

        if (codepoint > 0xFFFF) {
            codepoint -= 0x10000;
            result += (codepoint >>> 10 & 0x3FF | minHighSurrogate).fromCodePoint();
            codepoint = minLowSurrogate | codepoint & 0x3FF;
        }

        result += codepoint.fromCodePoint();

        return result;
    }

