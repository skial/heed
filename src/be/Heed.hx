@:nullSafety(Strict) class Heed {

    public static final asciiWhitespace:EReg = ~/[\x01-\x7F]/g;
    
    public static final escapeMap:Map<String, String> = ['"' => quot, '&' => amp, '\'' => '&#x27;', '<' => lt, '>' => gt, '`' => '&#x60;'];

    public static inline function escape(value:String):String {
        return ~/["&'<>`]/g.map( value, @:nullSafety(Off) ereg -> escapeMap.get(ereg.matched(0) ) );
    }

    public static function encode(value:String, everything:Bool = false, named:Bool = false, decimal:Bool = false, unsafe:Bool = false):String {
        if (everything) {
            value = asciiWhitespace.map(value, @:nullSafety(Off) function(ereg) {
                var symbol = ereg.matched(0).charCodeAt(0);
                if (named && encodeMap.exists('$symbol')) return encodeMap.get('$symbol');
                return decimal ? (symbol:Decimal) : (symbol:Hex);
            });

            if (named) {
                value = ~/&gt;\u20D2/g.replace(value, nvgt);
                value = ~/&lt;\u20D2/g.replace(value, nvlt);
                value = ~/&#x66;&#x6A;/g.replace(value, fjlig);
                
            }

            if (named) {
                value = regexEncodeNonAscii.map( value, ereg -> @:nullSafety(Off) encodeMap.get( '${ereg.matched(0).codePointAt(0)}' ) );
                
            }

        } else if (named) {
            if (!unsafe) {
                value = ~/["&'<>`]/g.map( value, ereg -> @:nullSafety(Off) encodeMap.get( '${ereg.matched(0).codePointAt(0)}' ) );

            }

            value = regexEncodeNonAscii.map( value, ereg -> @:nullSafety(Off) encodeMap.get( '${ereg.matched(0).codePointAt(0)}' ) );

        } else if (!unsafe) {
            value = ~/["&'<>`]/g.map( value, function(ereg) {
                var symbol = ereg.matched(0).codePointAt(0);
                return decimal ? (symbol:Decimal) : (symbol:Hex);
            } );

        }
        
        value = ~/[\uD800-\uDBFF][\uDC00-\uDFFF]/g.map( value, @:nullSafety(Off) function(ereg) {
            var char = ereg.matched(0).codePointAt(0);
            var codepoint = char;
            return decimal ? (codepoint:Decimal) : (codepoint:Hex);
        });
        
        value = ~/[\x01-\t\x0B\x0C\x0E-\x1F\x7F\x81\x8D\x8F\x90\x9D\xA0-\uFFFF]/g.map( value, function(ereg) {
            var symbol = ereg.matched(0).codePointAt(0);
            return decimal ? (symbol:Decimal) : (symbol:Hex);
        } );
        
        return value;
    }

    public static function decode(value:String, isAttribute:Bool = false, strict:Bool = false):String {
        return regexDecode.map( value, @:nullSafety(Off) function(ereg) {
            var ref = null;
            var next = null;
            var digit = null;
            //trace( ereg.matched(0), ereg.matched(1), ereg.matched(2), ereg.matched(3), ereg.matched(4), ereg.matched(5), ereg.matched(6), ereg.matched(7)  );

            if ((ref = ereg.matched(1)) != null) {
                return (cast ('&$ref;'):HtmlEntity).asCodePoints().fromCodePoints();

            }

            if ((ref = ereg.matched(2)) != null) {
                next = ereg.matched(3);

                if (next != null && next != '' && isAttribute) {
                    if (strict && next == '=') {
                        throw '`&` did not start a character reference';
                    }
                    return ereg.matched(0);

                } else {
                    if (strict) throw 'named character reference was not terminated by a semicolon';
                    return decodeMapLegacy.get('&$ref;').fromCodePoints().toString() + (next != null ? next : '');

                }

            }

            if ((digit = ereg.matched(4)) != null) {
                // Decode decimal escapes, e.g. `&#119558;`.
                var simi = ereg.matched(5);
                if (strict && (semi == null || semi == '')) throw 'character reference was not terminated by a semilcolon';
                var codepoint = Std.parseInt(digit);
                return codePointToSymbol(codepoint, strict);
            }
            
            if ((digit = ereg.matched(6)) != null) {
                // Decode hexadecimal escapes, e.g. `&#x1D306;`.
                var semi = ereg.matched(7);
                if (strict && (semi == null || semi == '')) throw 'character reference was not terminated by a semilcolon';
                var codepoint = Std.parseInt('0x$digit');
                return codePointToSymbol(codepoint, strict);
            }

            if (strict) throw 'named character reference was not terminated by a semicolon';
            
            return ereg.matched(0);
        } );
    }

    private static function codePointToSymbol(codepoint:Int, strict:Bool = false):String {
        var result = '';

        if ((codepoint >= minHighSurrogate && codepoint <= maxLowSurrogate) || codepoint > maxCodePoint) {
            if (strict) {
                throw 'character reference outside the permissible Unicode range';
            }
            return '\uFFFD';
        }

        if (decodeMapNumeric.exists(codepoint)) @:nullSafety(Off) {
            if (strict) {
                throw 'disallowed character reference';
            }
            return decodeMapNumeric.get(codepoint);
        }

        if (strict && invalidReferenceCodePoints.indexOf(codepoint) > -1) {
            throw 'disallowed character reference';
        }

        if (codepoint > 0xFFFF) {
            codepoint -= 0x10000;
            result += (codepoint >>> 10 & 0x3FF | minHighSurrogate).fromCodePoint();
            codepoint = minLowSurrogate | codepoint & 0x3FF;
        }

        result += codepoint.fromCodePoint();

        return result;
    }

